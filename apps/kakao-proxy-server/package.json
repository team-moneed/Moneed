{
    "name": "@moneed/kakao-proxy-server",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
        "build": "tsc && tsc-alias",
