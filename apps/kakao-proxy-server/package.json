        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.15.21",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "cross-env": "^10.0.0",
