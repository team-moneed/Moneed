{
    "name": "@moneed/db",
    "version": "0.1.0",
    "description": "prisma and prisma client",
    "license": "ISC",
    "author": "",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "default": "./dist/src/index.js"
        },
        "./generated": {
            "types": "./prisma/generated/prisma-client-js/index.d.ts",
            "default": "./prisma/generated/prisma-client-js/index.js"
        },
        "./prisma/generated/prisma-client-js": {
            "types": "./prisma/generated/prisma-client-js/index.d.ts",
            "default": "./prisma/generated/prisma-client-js/index.js"
        }
    },
    "scripts": {
        "build": "tsc && tsc-alias",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev --skip-generate",
        "db:deploy": "prisma migrate deploy",
        "type-check": "tsc --noEmit"
    },
    "prisma": {
        "seed": "tsx prisma/post.seed.ts"
    },
    "dependencies": {
        "@moneed/utility-types": "*",
        "@prisma/client": "^6.14.0"
    },
    "devDependencies": {
        "prisma": "^6.14.0",
        "tsc-alias": "^1.8.16"
    }
}
